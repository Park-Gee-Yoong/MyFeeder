{
  "name": "park-gee-yoong/myfeeder",
  "description": "Laravel wrapper for NeoFeeder API",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/Park-Gee-Yoong/MyFeeder",
  "authors": [
    {
      "name": "Park Gee Yoong"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0 || ^9.5",
    "mockery/mockery": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "ParkGeeYoong\\MyFeeder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParkGeeYoong\\MyFeeder\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ParkGeeYoong\\MyFeeder\\NeoFeederServiceProvider"
      ]
    }
  },
  "keywords": [
    "laravel",
    "neofeeder",
    "feeder",
    "api",
    "package"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
